<h1>Listing tipo_products</h1>

<table class="table table-bordered">
  <tr>
    <th>Name</th>
    <th>Descripcion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_products.each do |tipo_product| %>
  <tr>
    <td><%= tipo_product.name %></td>
    <td><%= tipo_product.descripcion %></td>
    <td><%= tipo_product.color %></td>
    <td><%= link_to 'Show', tipo_product %></td>
    <td><%= link_to 'Edit', edit_tipo_product_path(tipo_product) %></td>
    <td><%= link_to 'Destroy', tipo_product, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tipo product', new_tipo_product_path %>
